<table>
  <thead>
    <tr>
      <th>Code</th>
      <th>Name</th>
      <th>Action</th>
    </tr>
  </thead>
  <tbody id="projects">
  {{#each project in content}}
    <tr>
      <td>
          {{project.code}}
      </td>
      <td>
          {{project.name}}
      </td>
      <td>
        <a {{action showEditProject project}} href="#">Edit</a>
        <a {{action destroyProject project}} href="#">Delete</a>
      </td>
    </tr>
  {{/each}}
  </tbody>
</table>
{{outlet}}